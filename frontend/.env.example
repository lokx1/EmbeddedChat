# Frontend Environment Variables
# Copy this file to .env for development

# API Configuration
VITE_API_BASE_URL=http://localhost:8000

# Feature Flags
VITE_ENABLE_WORKFLOW_AUTOMATION=true
VITE_ENABLE_GOOGLE_INTEGRATION=true
VITE_ENABLE_AI_PROVIDERS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Debug Settings
VITE_DEBUG_API_CALLS=false
VITE_LOG_LEVEL=info
